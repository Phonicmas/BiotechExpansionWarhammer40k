    <!-- Custodes -->

    <GeneDef>
        <defName>BEWH_Custodes</defName>
        <label>custodes</label>
        <description>CUSTODESCODE1234567890Created by the Master of Mankind personally, these beings are what primarch are to a space marine, beings of immense power and skill.</description>        <!-- CUSTODESCODE1234567890 IS REQUIRED NO MATTER WHAT TRANSLATIONS -->
        <customEffectDescriptions>
            <li>CUSTOMSTART12345Considerably boosts most aspects of the pawnCUSTOMEND67890</li>            <!-- CUSTOMSTART12345 AND CUSTOMEND67890 IS REQUIRED NO MATTER WHAT TRANSLATIONS, WRITE BETWEEN THEM -->
        </customEffectDescriptions>
        <displayCategory>BEWHMankindsFinest</displayCategory>
        <displayOrderInCategory>1</displayOrderInCategory>
        <canGenerateInGeneSet>false</canGenerateInGeneSet>
        <biostatCpx>50</biostatCpx>
        <biostatMet>15</biostatMet>
        <biostatArc>10</biostatArc>
        <iconPath>UI/Icons/Genes/CustodesIcon</iconPath>
        <bodyType>Hulk</bodyType>
        <geneClass>Gene_Clotting</geneClass>
        <dontMindRawFood>true</dontMindRawFood>
        <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
        <immuneToToxGasExposure>true</immuneToToxGasExposure>
        <painFactor>0.01</painFactor>
        <sterilize>true</sterilize>
        <preventPermanentWounds>true</preventPermanentWounds>
        <overdoseChanceFactor>0</overdoseChanceFactor>
        <selectionWeight>0.1</selectionWeight>

        <statOffsets>
            <MaxHitPoints>850</MaxHitPoints>
            <ArmorRating_Blunt>1.30</ArmorRating_Blunt>
            <ArmorRating_Sharp>1.30</ArmorRating_Sharp>
            <ArmorRating_Heat>1.30</ArmorRating_Heat>
            <CarryingCapacity>760</CarryingCapacity>
            <ToxicEnvironmentResistance>2</ToxicEnvironmentResistance>
            <ToxicResistance>2</ToxicResistance>
            <MentalBreakThreshold>-1</MentalBreakThreshold>
            <ComfyTemperatureMin>-200</ComfyTemperatureMin>
            <ComfyTemperatureMax>200</ComfyTemperatureMax>
            <MeleeDodgeChance>60</MeleeDodgeChance>
        </statOffsets>

        <statFactors>
            <PsychicSensitivity>0</PsychicSensitivity>
            <Fertility>0</Fertility>
            <Mass>3</Mass>
            <InjuryHealingFactor>20</InjuryHealingFactor>
            <MoveSpeed>2</MoveSpeed>
            <RestFallRateFactor>0.05</RestFallRateFactor>
            <MeleeDamageFactor>6</MeleeDamageFactor>
            <AimingDelayFactor>0.2</AimingDelayFactor>
            <MeleeWeapon_CooldownMultiplier>0.2</MeleeWeapon_CooldownMultiplier>
            <RangedCooldownFactor>0.2</RangedCooldownFactor>
            <MeleeHitChance>4</MeleeHitChance>
            <StaggerDurationFactor>0</StaggerDurationFactor>
            <IncomingDamageFactor>0.35</IncomingDamageFactor>
            <RawNutritionFactor>4</RawNutritionFactor>
            <GlobalLearningFactor>3.5</GlobalLearningFactor>
            <ImmunityGainSpeed>8</ImmunityGainSpeed>
        </statFactors>

        <capMods>
            <li>
                <capacity>Consciousness</capacity>
                <postFactor>3.5</postFactor>
            </li>
            <li>
                <capacity>BloodPumping</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Manipulation</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Breathing</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Sight</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Hearing</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>BloodFiltration</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Eating</capacity>
                <postFactor>5</postFactor>
            </li>
            <li>
                <capacity>Talking</capacity>
                <postFactor>5</postFactor>
            </li>
        </capMods>

        <suppressedTraits>
            <li>
                <def>Industriousness</def>
                <degree>-1</degree>
            </li>
            <li>
                <def>Industriousness</def>
                <degree>-2</degree>
            </li>
            <li>
                <def>Beauty</def>
                <degree>-1</degree>
            </li>
            <li>
                <def>Beauty</def>
                <degree>-2</degree>
            </li>
            <li>
                <def>Immunity</def>
