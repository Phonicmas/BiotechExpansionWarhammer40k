<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Progenoid Gland -->

	<HediffDef>
		<defName>BEWH_ProgenoidGlandsGrowth</defName>
		<label>progenoid glands</label>
		<hediffClass>HediffWithComps</hediffClass>
		<description>Your progenoid glands are currently producing germ cells for new gene-seeds.</description>
		<everCurableByItem>false</everCurableByItem>
		<initialSeverity>0.001</initialSeverity>
		<maxSeverity>1</maxSeverity>
		<stages>
			<li>
				<label>minor growth</label>
			</li>
			<li>
				<minSeverity>0.65</minSeverity>
				<label>major growth</label>
			</li>
			<li>
				<minSeverity>1</minSeverity>
				<label>done growing</label>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>18</severityPerDay>
			</li>
		</comps>
	</HediffDef>

</Defs>