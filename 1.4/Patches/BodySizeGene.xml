<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Body Size Genes</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/GeneDef[defName="BEWH_Ossmodula"]/statFactors</xpath>
					<value>
						<bodySizeFactor>1.15</bodySizeFactor>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/GeneDef[defName="BEWH_Magnificat"]/statFactors</xpath>
					<value>
						<bodySizeFactor>1.2</bodySizeFactor>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>